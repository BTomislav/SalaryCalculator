<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#4B5359"></solid>
            <corners android:radius="5dp"></corners>
            <stroke android:color="#4B5359" android:width="1dp"></stroke>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp"></corners>
            <stroke android:color="#4B5359" android:width="1dp">
            </stroke>
            <solid android:color="#272B2E">
            </solid>
        </shape>
    </item>
</selector>